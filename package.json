{
	"name": "pixel-sort",
	"version": "1.0.0",
	"private": true,
	"description": "Liven your photos with a glitch effect",
	"keywords": [
		"pixel",
		"sort",
		"glitch",
		"image"
	],
	"homepage": "https://github.com/evelynhathaway/pixel-sort#readme",
	"bugs": {
		"url": "https://github.com/evelynhathaway/pixel-sort/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evelynhathaway/pixel-sort.git"
	},
	"license": "MIT",
	"author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
	"scripts": {
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"lint": "eslint ./",
		"start": "react-scripts start",
		"test": "react-scripts test"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
	},
	"lint-staged": {
		"**/package.json": "sort-package-json",
		"./": "eslint --fix"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@emotion/react": "^11.1.4",
		"@emotion/styled": "^11.0.0",
		"@material-ui/core": "^5.0.0-alpha.23",
		"clsx": "^1.1.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-dropzone": "^11.2.4"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.3",
		"@testing-library/user-event": "^12.6.2",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.22",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"commitlint": "^11.0.0",
		"conventional-changelog-evelyn": "^1.2.1",
		"eslint": "^7.18.0",
		"eslint-plugin-evelyn": "^4.0.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-regexp": "^0.1.1",
		"eslint-plugin-testing-library": "^3.10.1",
		"eslint-plugin-unicorn": "^23.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"node-sass": "^4.0.0",
		"react-scripts": "4.0.1",
		"sort-package-json": "^1.48.1",
		"typescript": "^4.1.3",
		"worker-loader": "^3.0.7"
	}
}
